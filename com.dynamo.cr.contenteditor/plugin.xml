<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.dynamo.cr.contenteditor.editors.CollectionEditor"
            contributorClass="com.dynamo.cr.contenteditor.editors.CollectionEditorActionBarContributor"
            default="false"
            extensions="collection"
            icon="icons/bricks.png"
            id="com.dynamo.cr.contenteditor.editors.CollectionEditor"
            name="Collection Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="com.dynamo.crepo.category"
            description="Activate Tool"
            id="com.dynamo.cr.contenteditor.commands.activateTool"
            name="Activate Tool">
         <commandParameter
               id="org.eclipse.ui.commands.radioStateParameter"
               name="tool"
               optional="false">
         </commandParameter>
         <state
               id="org.eclipse.ui.commands.radioState">
            <class
                  class="org.eclipse.ui.handlers.RadioState">
               <parameter
                     name="default"
                     value="select">
               </parameter>
               <parameter
                     name="persisted"
                     value="false">
               </parameter>
            </class>
         </state>
      </command>
      <command
            categoryId="com.dynamo.crepo.category"
            description="Parent objects"
            id="com.dynamo.cr.contenteditor.commands.parent"
            name="Parent objects">
      </command>
      <command
            categoryId="com.dynamo.crepo.category"
            description="Unparent objects"
            id="com.dynamo.cr.contenteditor.commands.unparent"
            name="Unparent objects">
      </command>
      <command
            categoryId="com.dynamo.crepo.category"
            description="Add GameObject"
            id="com.dynamo.cr.contenteditor.commands.addgameobject"
            name="Add GameObject">
      </command>
      <command
            categoryId="com.dynamo.crepo.category"
            description="Add Subcollection"
            id="com.dynamo.cr.contenteditor.commands.addsubcollection"
            name="Add Subcollection">
      </command>
      <command
            categoryId="com.dynamo.crepo.category"
            defaultHandler="com.dynamo.cr.contenteditor.commands.Delete"
            description="Delete"
            id="com.dynamo.cr.contenteditor.commands.delete"
            name="Delete">
      </command>
      <command
            categoryId="com.dynamo.crepo.category"
            defaultHandler="com.dynamo.cr.contenteditor.commands.Copy"
            description="Copy"
            id="com.dynamo.cr.contenteditor.commands.copy"
            name="Copy">
      </command>
      <command
            categoryId="com.dynamo.crepo.category"
            defaultHandler="com.dynamo.cr.contenteditor.commands.Paste"
            description="Paste"
            id="com.dynamo.cr.contenteditor.commands.paste"
            name="Paste">
      </command>
      <command
            categoryId="com.dynamo.crepo.category"
            description="Activate Camera"
            id="com.dynamo.cr.contenteditor.commands.activateCamera"
            name="Activate Camera">
         <commandParameter
               id="org.eclipse.ui.commands.radioStateParameter"
               name="camera"
               optional="false">
         </commandParameter>
         <state
               id="org.eclipse.ui.commands.radioState">
            <class
                  class="org.eclipse.ui.handlers.RadioState">
               <parameter
                     name="default"
                     value="orthographic">
               </parameter>
               <parameter
                     name="persisted"
                     value="false">
               </parameter>
            </class>
         </state>
      </command>
      <command
            categoryId="com.dynamo.crepo.category"
            description="Activate Orientation"
            id="com.dynamo.cr.contenteditor.commands.activateOrientation"
            name="Activate Orientation">
         <commandParameter
               id="org.eclipse.ui.commands.radioStateParameter"
               name="orientation"
               optional="false">
         </commandParameter>
         <state
               id="org.eclipse.ui.commands.radioState">
            <class
                  class="org.eclipse.ui.handlers.RadioState">
               <parameter
                     name="default"
                     value="local">
               </parameter>
               <parameter
                     name="persisted"
                     value="false">
               </parameter>
            </class>
         </state>
      </command>
      <command
            categoryId="com.dynamo.crepo.category"
            defaultHandler="com.dynamo.cr.contenteditor.commands.Cut"
            description="Cut"
            id="com.dynamo.cr.contenteditor.commands.cut"
            name="Cut">
      </command>
      <command
            categoryId="com.dynamo.crepo.category"
            description="Reset Camera"
            id="com.dynamo.cr.contenteditor.commands.resetCamera"
            name="Reset Camera">
      </command>
      <command
            categoryId="com.dynamo.crepo.category"
            description="Frame Objects"
            id="com.dynamo.cr.contenteditor.commands.frameObjects"
            name="Frame Object">
      </command>
      <command
            categoryId="com.dynamo.crepo.category"
            description="Activate Pivot"
            id="com.dynamo.cr.contenteditor.commands.activatePivot"
            name="Activate Pivot">
         <commandParameter
               id="org.eclipse.ui.commands.radioStateParameter"
               name="pivot"
               optional="false">
         </commandParameter>
         <state
               id="org.eclipse.ui.commands.radioState">
            <class
                  class="org.eclipse.ui.handlers.RadioState">
               <parameter
                     name="default"
                     value="manipulator">
               </parameter>
               <parameter
                     name="persisted"
                     value="false">
               </parameter>
            </class>
         </state>
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu?after=edit">
         <menu
               label="Collection">
            <command
                  commandId="com.dynamo.cr.contenteditor.commands.addgameobject"
                  icon="icons/add_att_16x16.gif"
                  label="Add Game Object"
                  style="push">
            </command>
            <command
                  commandId="com.dynamo.cr.contenteditor.commands.addsubcollection"
                  icon="icons/add_att_16x16.gif"
                  label="Add Sub Collection"
                  style="push">
            </command>
            <command
                  commandId="com.dynamo.cr.contenteditor.commands.parent"
                  icon="icons/bricks.png"
                  label="Parent"
                  style="push">
            </command>
            <command
                  commandId="com.dynamo.cr.contenteditor.commands.unparent"
                  icon="icons/brick.png"
                  label="Unparent"
                  style="push">
            </command>
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="com.dynamo.cr.contenteditor.definitions.collectionEditorActive">
               </reference>
            </visibleWhen>
            <separator
                  name="com.dynamo.cr.contenteditor.menus.collection.separator1"
                  visible="true">
            </separator>
            <menu
                  label="Camera">
               <command
                     commandId="com.dynamo.cr.contenteditor.commands.resetCamera"
                     label="Reset Camera"
                     style="push">
               </command>
               <command
                     commandId="com.dynamo.cr.contenteditor.commands.frameObjects"
                     label="Frame Objects"
                     style="push">
               </command>
               <menu
                     label="Mode">
                  <command
                        commandId="com.dynamo.cr.contenteditor.commands.activateCamera"
                        icon="icons/box.png"
                        label="Perspective"
                        style="radio">
                     <parameter
                           name="org.eclipse.ui.commands.radioStateParameter"
                           value="perspective">
                     </parameter>
                  </command>
                  <command
                        commandId="com.dynamo.cr.contenteditor.commands.activateCamera"
                        icon="icons/box_closed.png"
                        label="Orthographic"
                        style="radio">
                     <parameter
                           name="org.eclipse.ui.commands.radioStateParameter"
                           value="orthographic">
                     </parameter>
                  </command>
               </menu>
            </menu>
            <separator
                  name="com.dynamo.cr.contenteditor.menus.collection.separator2"
                  visible="true">
            </separator>
            <menu
                  label="Tools">
               <command
                     commandId="com.dynamo.cr.contenteditor.commands.activateTool"
                     icon="icons/select.png"
                     label="Select"
                     style="radio">
                  <parameter
                        name="org.eclipse.ui.commands.radioStateParameter"
                        value="select">
                  </parameter>
               </command>
               <command
                     commandId="com.dynamo.cr.contenteditor.commands.activateTool"
                     icon="icons/move.png"
                     label="Move"
                     style="radio">
                  <parameter
                        name="org.eclipse.ui.commands.radioStateParameter"
                        value="move">
                  </parameter>
               </command>
               <command
                     commandId="com.dynamo.cr.contenteditor.commands.activateTool"
                     icon="icons/rotate.png"
                     label="Rotate"
                     style="radio">
                  <parameter
                        name="org.eclipse.ui.commands.radioStateParameter"
                        value="rotate">
                  </parameter>
               </command>
               <menu
                     label="Orientation">
                  <command
                        commandId="com.dynamo.cr.contenteditor.commands.activateOrientation"
                        icon="icons/local.png"
                        label="Local"
                        style="radio">
                     <parameter
                           name="org.eclipse.ui.commands.radioStateParameter"
                           value="local">
                     </parameter>
                  </command>
                  <command
                        commandId="com.dynamo.cr.contenteditor.commands.activateOrientation"
                        icon="icons/global.png"
                        label="Global"
                        style="radio">
                     <parameter
                           name="org.eclipse.ui.commands.radioStateParameter"
                           value="global">
                     </parameter>
                  </command>
               </menu>
               <menu
                     label="Pivot">
                  <command
                        commandId="com.dynamo.cr.contenteditor.commands.activatePivot"
                        icon="icons/pivot_unique.png"
                        label="Local"
                        style="radio">
                     <parameter
                           name="org.eclipse.ui.commands.radioStateParameter"
                           value="local">
                     </parameter>
                  </command>
                  <command
                        commandId="com.dynamo.cr.contenteditor.commands.activatePivot"
                        icon="icons/pivot_common.png"
                        label="Global"
                        style="radio">
                     <parameter
                           name="org.eclipse.ui.commands.radioStateParameter"
                           value="manipulator">
                     </parameter>
                  </command>
               </menu>
            </menu>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="com.dynamo.cr.contenteditor.commands.activateTool"
            contextId="com.dynamo.cr.contenteditor.contexts.collectioneditor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Q">
         <parameter
               id="org.eclipse.ui.commands.radioStateParameter"
               value="select">
         </parameter>
      </key>
      <key
            commandId="com.dynamo.cr.contenteditor.commands.activateTool"
            contextId="com.dynamo.cr.contenteditor.contexts.collectioneditor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="W">
         <parameter
               id="org.eclipse.ui.commands.radioStateParameter"
               value="move">
         </parameter>
      </key>
      <key
            commandId="com.dynamo.cr.contenteditor.commands.activateTool"
            contextId="com.dynamo.cr.contenteditor.contexts.collectioneditor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="E">
         <parameter
               id="org.eclipse.ui.commands.radioStateParameter"
               value="rotate">
         </parameter>
      </key>
      <key
            commandId="com.dynamo.cr.contenteditor.commands.parent"
            contextId="com.dynamo.cr.contenteditor.contexts.collectioneditor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="P">
      </key>
      <key
            commandId="com.dynamo.cr.contenteditor.commands.unparent"
            contextId="com.dynamo.cr.contenteditor.contexts.collectioneditor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="U">
      </key>
      <key
            commandId="com.dynamo.cr.contenteditor.commands.addgameobject"
            contextId="com.dynamo.cr.contenteditor.contexts.collectioneditor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="I">
      </key>
      <key
            commandId="com.dynamo.cr.contenteditor.commands.addsubcollection"
            contextId="com.dynamo.cr.contenteditor.contexts.collectioneditor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="O">
      </key>
      <key
            commandId="com.dynamo.cr.contenteditor.commands.activateCamera"
            contextId="com.dynamo.cr.contenteditor.contexts.collectioneditor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="SPACE">
         <parameter
               id="org.eclipse.ui.commands.radioStateParameter"
               value="next">
         </parameter>
      </key>
      <key
            commandId="com.dynamo.cr.contenteditor.commands.activateOrientation"
            contextId="com.dynamo.cr.contenteditor.contexts.collectioneditor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="TAB">
         <parameter
               id="org.eclipse.ui.commands.radioStateParameter"
               value="next">
         </parameter>
      </key>
      <key
            commandId="com.dynamo.cr.contenteditor.commands.frameObjects"
            contextId="com.dynamo.cr.contenteditor.contexts.collectioneditor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="F">
      </key>
      <key
            commandId="com.dynamo.cr.contenteditor.commands.activatePivot"
            contextId="com.dynamo.cr.contenteditor.contexts.collectioneditor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence=".">
         <parameter
               id="org.eclipse.ui.commands.radioStateParameter"
               value="next">
         </parameter>
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.contexts">
      <context
            id="com.dynamo.cr.contenteditor.contexts.collectioneditor"
            name="Collection Editor">
      </context>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.dynamo.cr.contenteditor.commands.Delete"
            commandId="org.eclipse.ui.edit.delete">
         <activeWhen>
            <reference
                  definitionId="com.dynamo.cr.contenteditor.definitions.collectionEditorActive">
            </reference>
         </activeWhen>

      </handler>
      <handler
            class="com.dynamo.cr.contenteditor.commands.Copy"
            commandId="org.eclipse.ui.edit.copy">
         <activeWhen>
            <reference
                  definitionId="com.dynamo.cr.contenteditor.definitions.collectionEditorActive">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="com.dynamo.cr.contenteditor.commands.Paste"
            commandId="org.eclipse.ui.edit.paste">
         <activeWhen>
            <reference
                  definitionId="com.dynamo.cr.contenteditor.definitions.collectionEditorActive">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="com.dynamo.cr.contenteditor.commands.Parent"
            commandId="com.dynamo.cr.contenteditor.commands.parent">
         <activeWhen>
            <reference
                  definitionId="com.dynamo.cr.contenteditor.definitions.collectionEditorActive">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="com.dynamo.cr.contenteditor.commands.Unparent"
            commandId="com.dynamo.cr.contenteditor.commands.unparent">
         <activeWhen>
            <reference
                  definitionId="com.dynamo.cr.contenteditor.definitions.collectionEditorActive">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="com.dynamo.cr.contenteditor.commands.AddGameObject"
            commandId="com.dynamo.cr.contenteditor.commands.addgameobject">
         <activeWhen>
            <reference
                  definitionId="com.dynamo.cr.contenteditor.definitions.collectionEditorActive">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="com.dynamo.cr.contenteditor.commands.AddSubCollection"
            commandId="com.dynamo.cr.contenteditor.commands.addsubcollection">
         <activeWhen>
            <reference
                  definitionId="com.dynamo.cr.contenteditor.definitions.collectionEditorActive">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="com.dynamo.cr.contenteditor.commands.ActivateTool"
            commandId="com.dynamo.cr.contenteditor.commands.activateTool">
         <activeWhen>
            <reference
                  definitionId="com.dynamo.cr.contenteditor.definitions.collectionEditorActive">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="com.dynamo.cr.contenteditor.commands.ActivateCamera"
            commandId="com.dynamo.cr.contenteditor.commands.activateCamera">
         <activeWhen>
            <reference
                  definitionId="com.dynamo.cr.contenteditor.definitions.collectionEditorActive">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="com.dynamo.cr.contenteditor.commands.ActivateOrientation"
            commandId="com.dynamo.cr.contenteditor.commands.activateOrientation">
         <activeWhen>
            <reference
                  definitionId="com.dynamo.cr.contenteditor.definitions.collectionEditorActive">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="com.dynamo.cr.contenteditor.commands.Cut"
            commandId="org.eclipse.ui.edit.cut">
         <activeWhen>
            <reference
                  definitionId="com.dynamo.cr.contenteditor.definitions.collectionEditorActive">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="com.dynamo.cr.contenteditor.commands.ResetCamera"
            commandId="com.dynamo.cr.contenteditor.commands.resetCamera">
         <activeWhen>
            <reference
                  definitionId="com.dynamo.cr.contenteditor.definitions.collectionEditorActive">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="com.dynamo.cr.contenteditor.commands.FrameObjects"
            commandId="com.dynamo.cr.contenteditor.commands.frameObjects">
         <activeWhen>
            <reference
                  definitionId="com.dynamo.cr.contenteditor.definitions.collectionEditorActive">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="com.dynamo.cr.contenteditor.commands.ActivatePivot"
            commandId="com.dynamo.cr.contenteditor.commands.activatePivot">
         <activeWhen>
            <reference
                  definitionId="com.dynamo.cr.contenteditor.definitions.collectionEditorActive">
            </reference>
         </activeWhen>
      </handler>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="com.dynamo.cr.contenteditor.definitions.collectionEditorActive">
         <with
               variable="activeContexts">
            <iterate
                  ifEmpty="false"
                  operator="or">
               <equals
                     value="com.dynamo.cr.contenteditor.contexts.collectioneditor">
               </equals>
            </iterate>
         </with>
      </definition>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="com.dynamo.rclient.preferences.PreferencePage"
            class="com.dynamo.cr.contenteditor.preferences.PreferencePage"
            id="com.dynamo.cr.contenteditor.preferences.preferencePage"
            name="Collection Editor">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.dynamo.cr.contenteditor.preferences.PreferenceInitializer">
      </initializer>
   </extension>

</plugin>
