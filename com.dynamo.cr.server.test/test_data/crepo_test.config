BranchRoot: "tmp/branch_root"
Filters: ".*?\\.git"
ServicePort: 6500
BuildDataPort: 6001
PersistenceUnitName: "unit-test"

projects {
  Id: "proj1"
  Name: "Project 1"
  Root: "tmp/test_data/proj1"

  ProgressRe: "\\[(\\d+)/(\\d+)\\]"

  ConfigureCommand: {
    Args: "waf"
    Args: "configure"
  }

  BuildCommand: {
    Args: "waf"
  }

  RebuildCommand: {
    Args: "waf" Args: "clean" Args: "build"
  }

  ExeCommand: {}

}

