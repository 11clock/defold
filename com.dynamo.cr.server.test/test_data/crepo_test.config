hostname: "localhost"
branch_root: "tmp/branch_root"
filters: ".*?\\.git"
service_port: 6500
build_data_port: 6001
persistence_unit_name: "unit-test"
repository_root: "tmp/test_data"
progress_re: "\\[(\\d+)/(\\d+)\\]"
smtp_host: ""
smtp_username: ""
smtp_password: ""

registration_template {
    from: "noreply@defold.se"
    subject: "registration"
    message: "Registration message"
}

invitation_template {
    from: "noreply@defold.se"
    subject: "invitation"
    message: "${key}"
}

invitation_count_map { key: 1 value: 2 }

applications: {
  platform: "linux"
  name: "fake_engine"
  path: "test_data/fake_engine"
}

configure_command: {
  args: "{cwd}/scripts/configure.sh"
  args: "configure"
}

build_command: {
  args: "{cwd}/tmp/build.sh"
}

rebuild_command: {
  args: "{cwd}/tmp/build.sh"
  args: "clean"
  args: "build"
}

exe_command {}

project_templates {
    id: "proj1"
    path: "tmp/templates/proj1"
    description: "Project 1"
}
