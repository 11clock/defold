function update(self)
    assert(is_visible(vec_math.vector3(0,0,0), vec_math.vector3(0,0,0)), "origin")
    assert(is_visible(vec_math.vector3(-1,-1,-1), vec_math.vector3(1, 1, 1)), "edge case within")
    assert(not is_visible(vec_math.vector3(-1.1,-1,-1), vec_math.vector3(1, 1, 1)), "edge case outside")
    assert(not is_visible(vec_math.vector3(-1,-1.1,-1), vec_math.vector3(1, 1, 1)), "edge case outside")
    assert(not is_visible(vec_math.vector3(-1,-1,-1.1), vec_math.vector3(1, 1, 1)), "edge case outside")
    assert(not is_visible(vec_math.vector3(-1,-1,-1), vec_math.vector3(1.1, 1, 1)), "edge case outside")
    assert(not is_visible(vec_math.vector3(-1,-1,-1), vec_math.vector3(1, 1.1, 1)), "edge case outside")
    assert(not is_visible(vec_math.vector3(-1,-1,-1), vec_math.vector3(1, 1, 1.1)), "edge case outside")
    assert(is_visible(vec_math.vector3(-1.1,-1,-1), vec_math.vector3(1, 1, 1), 1.1), "edge case margin within")
    assert(not is_visible(vec_math.vector3(-1.2,-1,-1), vec_math.vector3(1, 1, 1), 1.1), "edge case margin outside")
end
