function update(self)
    assert(go.is_visible(vmath.vector3(0,0,0), vmath.vector3(0,0,0)), "origin")
    assert(go.is_visible(vmath.vector3(-1,-1,-1), vmath.vector3(1, 1, 1)), "edge case within")
    assert(not go.is_visible(vmath.vector3(-1.1,-1,-1), vmath.vector3(1, 1, 1)), "edge case outside")
    assert(not go.is_visible(vmath.vector3(-1,-1.1,-1), vmath.vector3(1, 1, 1)), "edge case outside")
    assert(not go.is_visible(vmath.vector3(-1,-1,-1.1), vmath.vector3(1, 1, 1)), "edge case outside")
    assert(not go.is_visible(vmath.vector3(-1,-1,-1), vmath.vector3(1.1, 1, 1)), "edge case outside")
    assert(not go.is_visible(vmath.vector3(-1,-1,-1), vmath.vector3(1, 1.1, 1)), "edge case outside")
    assert(not go.is_visible(vmath.vector3(-1,-1,-1), vmath.vector3(1, 1, 1.1)), "edge case outside")
    assert(go.is_visible(vmath.vector3(-1.1,-1,-1), vmath.vector3(1, 1, 1), 1.1), "edge case margin within")
    assert(not go.is_visible(vmath.vector3(-1.2,-1,-1), vmath.vector3(1, 1, 1), 1.1), "edge case margin outside")
end
