function init(self)
    go.post("load_collection", { collection = "test.collectionc" })
    go.post("activate_collection", { collection = "test.collectionc" })
    self.timer = 4
end

function update(self)
    self.timer = self.timer - go.dt
    if self.timer < 0 then
        go.post("exit", { code = 1 })
    end
end
