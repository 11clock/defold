branch_root: "tmp/branch_root"
filters: ".*?\\.git"
service_port: 6500
build_data_port: 6001

projects {
  id: "proj1"
  name: "Project 1"
  root: "tmp/test_data/proj1"

  progress_re: "\\[(\\d+)/(\\d+)\\]"

  configure_command: {
    args: "waf"
    args: "configure"
  }

  build_command: {
    args: "waf"
  }

  rebuild_command: {
    args: "waf" args: "clean" args: "build"
  }

  exe_command: {}

}

