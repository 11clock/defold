branch_root: "/tmp/br"
filters: ".*?\\.git"
service_port: 9998
build_data_port: 6000

projects {
  id: "jetbot"
  name: "Jetbot"
  root: "/home/ragnar/dynamo/jetbot"
  progress_re: "\\[(\\d+)/(\\d+)\\]"

  applications: {
    platform: "darwin"
    name: "jetbot"
    path: "/Users/chmu/Projects/dynamo/jetbot/build/default/src/jetbot"
  }

  configure_command: {
    args: "waf"
    args: "configure"
  }

  build_command: {
    args: "waf"
  }

  rebuild_command: {
    args: "waf" args: "clean" args: "build"
  }

  exe_command: {
    args: "{executable}"
    args: "--config=resource.uri=http://localhost:6000/jetbot/{user}/{branch}/build/default/content"
    args: "http://localhost:6000/jetbot/{user}/{branch}/build/default/content/game.projectc"
  }
}

