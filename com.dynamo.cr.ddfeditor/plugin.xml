            id="com.dynamo.cr.ddfeditor.resourcetypes.InputBinding"
            name="Input Binding"
            proto-message-class="com.dynamo.input.proto.Input$InputBinding"
            template-data=" ">
      </resource-type>
      <resource-type
            file-extension="gamepads"
            id="com.dynamo.cr.ddfeditor.resourcetypes.Gamepads"
            name="Gamepads"
            proto-message-class="com.dynamo.input.proto.Input$GamepadMaps"
            template-data="  ">
      </resource-type>
      <resource-type
            file-extension="collection"
            id="com.dynamo.cr.ddfeditor.resourcetypes.Collection"
            name="Collection"
            proto-message-class="com.dynamo.gameobject.proto.GameObject$CollectionDesc"
            template-data="Name: 'unnamed'">
      </resource-type>
      <resource-type
            edit-support-class="com.dynamo.cr.ddfeditor.resourcetypes.EmitterEditSupport"
            file-extension="emitter"
            id="com.dynamo.cr.ddfeditor.resourcetypes.Emitter"
            name="Emitter"
            proto-message-class="com.dynamo.particle.proto.Particle$Emitter"
            template-data="Mode: PLAY_MODE_ONCE Duration: 0.2 Space: EMISSION_SPACE_WORLD Rotation { X: 0.0 Y: 0.0 Z: 0.0 W: 1.0 } Texture { Name: &apos;unnamed&apos; TX: 1 TY: 1 } Material: &apos;unnamed&apos; MaxParticleCount: 8 Type: EMITTER_TYPE_CONE">
      </resource-type>
      <resource-type
            file-extension="sprite"
            id="com.dynamo.cr.ddfeditor.resourcetypes.Sprite"
            name="Sprite"
            proto-message-class="com.dynamo.sprite.proto.Sprite$SpriteDesc"
            template-data="Texture: &apos;&apos; Width: 1 Height: 1 Height: 1">
      </resource-type>
      <resource-type
