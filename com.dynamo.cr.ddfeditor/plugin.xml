            proto-message-class="com.dynamo.gui.proto.Gui$SceneDesc"
            template-data="Script: &apos;&apos;">
      </resource-type>
      <resource-type
            file-extension="font"
            id="com.dynamo.cr.ddfeditor.resourcetypes.Font"
            name="Font"
            proto-message-class="com.dynamo.render.proto.Font$FontDesc"
            template-data="Font: '' Material: '' Size: 15">
      </resource-type>
      <resource-type
            edit-support-class="com.dynamo.cr.ddfeditor.resourcetypes.MaterialEditSupport"
            file-extension="material"
            id="com.dynamo.cr.ddfeditor.resourcetypes.Material"
            name="Material"
            proto-message-class="com.dynamo.render.proto.Material$MaterialDesc"
            template-data="Name: 'unnamed' VertexProgram: '' FragmentProgram: ''">
      </resource-type>
      <resource-type
            edit-support-class="com.dynamo.cr.ddfeditor.resourcetypes.RenderEditSupport"
            file-extension="render"
            id="com.dynamo.cr.ddfeditor.resourcetypes.Render"
            name="Render"
            proto-message-class="com.dynamo.render.proto.Render$RenderPrototypeDesc"
            template-data="Script: &apos;&apos;">
      </resource-type>
      <resource-type
            edit-support-class="com.dynamo.cr.ddfeditor.resourcetypes.InputBindingEditSupport"
