emitters: {
    mode:               PLAY_MODE_LOOP
    space:              EMISSION_SPACE_WORLD
    position:           { x: 0 y: 0 z: 0 }
    rotation:           { x: 0 y: 0 z: 0 w: 1 }

    tile_source:        "particle.tilesource"
    animation:          ""
    material:           "particle.material"
    duration:           1

    max_particle_count: 1

    type:               EMITTER_TYPE_CONE

    properties {
        key: EMITTER_KEY_SPAWN_RATE
        points { x: 0.0 y: 4.0 t_x: 1 t_y: 0 }
    }
    properties {
        key: EMITTER_KEY_PARTICLE_SIZE
        points { x: 0.0 y: 0.0 t_x: 0.70710677 t_y: 0.70710677 }
        points { x: 1.0 y: 1.0 t_x: 0.70710677 t_y: 0.70710677 }
    }
    properties {
        key: EMITTER_KEY_PARTICLE_LIFE_TIME
        points { x: 0.0 y: 0.25 t_x: 1 t_y: 0 }
        points { x: 1.0 y: 0.25 t_x: 1 t_y: 0 }
    }
}
