<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>
        .important {
            font-weight: bold;
        }
    </ui:style>
    <g:HTMLPanel>
        <div style="background-color: #eee; width: 200px; margin: 16px; padding: 16px;">
            <g:Anchor ui:field="name"></g:Anchor>
	        <g:Label ui:field="memberOrOwner"></g:Label>
	        <g:Label ui:field="description"></g:Label>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>

