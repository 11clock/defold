struct Vertex
{
    float4 position : ATTR0;
    float3 normal : ATTR1;
    float2 uv : ATTR2;
};

struct Fragment
{
    float4 position : POSITION;
    float2 uv : TEXCOORD0;
};

void main(in Vertex v_in,
          out Fragment f_out)
{
    f_out.position = v_in.position;
    f_out.uv = v_in.uv;
}
